			<Текст>// Выборка регистраторов из заданных регистров для Договоров

СЗ = Новый СхемаЗапроса;

// Добавляем выборку во временную таблицу
Пакет = СЗ.ПакетЗапросов.Добавить();
Пакет.ТаблицаДляПомещения = "ВТ";
ПервыйИсточник = Истина;
Для каждого ПараметрыИсточника из ПараметрыИсточников Цикл
	
	Если ТипЗнч(ПараметрыИсточника) = Тип("Строка") Тогда
		// Простое описание переделываем в полное
		ПараметрыИсточника = Новый Структура("Источник, ИмяПоляОтбора, ИмяПараметраМассива, ДопУсловия", 
			ПараметрыИсточника);
	КонецЕсли;
	
	Если ПервыйИсточник Тогда
		Оператор = Пакет.Операторы[0];
		ПервыйИсточник = Ложь;
	Иначе
		Оператор = Пакет.Операторы.Добавить();
	КонецЕсли;
	
	Оператор.Источники.Добавить(ПараметрыИсточника.Источник);
	Оператор.ВыбираемыеПоля.Добавить("ВЫРАЗИТЬ(Регистратор КАК Документ." + ИмяПКО + ")");
	
	ИмяПоляОтбора = ?(ЗначениеЗаполнено(ПараметрыИсточника.ИмяПоляОтбора), ПараметрыИсточника.ИмяПоляОтбора, "Договор");
	ИмяПараметраМассива = ?(ЗначениеЗаполнено(ПараметрыИсточника.ИмяПараметраМассива), ПараметрыИсточника.ИмяПараметраМассива, "Договоры");
	Оператор.Отбор.Добавить(ИмяПоляОтбора + " В(&amp;" + ИмяПараметраМассива + ")");
	
	Если ЗначениеЗаполнено(ПараметрыИсточника.ДопУсловия) Тогда
		Оператор.Отбор.Добавить(ПараметрыИсточника.ДопУсловия);
	КонецЕсли;
	//Если СтрНачинаетсяС(ПараметрыИсточника.Источник, "РегистрНакопления") Тогда
		Оператор.Отбор.Добавить("Активность");
	//КонецЕсли;
КонецЦикла;
Пакет.Колонки[0].Псевдоним = "Ссылка";

// Добавляем окончательную выборку
Пакет = СЗ.ПакетЗапросов.Добавить();
Оператор = Пакет.Операторы[0];
Оператор.Источники.Добавить("Документ." + ИмяПКО);
Оператор.ВыбираемыеПоля.Добавить("&amp;ЗП_Звездочка");
Оператор.Отбор.Добавить("Ссылка В (ВЫБРАТЬ ВТ.Ссылка ИЗ ВТ)");

Текст = СЗ.ПолучитьТекстЗапроса();

Текст = СтрЗаменить(Текст, "&amp;ЗП_Звездочка КАК ЗП_Звездочка", "*");

Запрос = Новый Запрос(Текст);
Запрос.УстановитьПараметр("Договоры", Параметры.Договоры);

ВыборкаДанных = Запрос;
Выполнить(Алгоритмы.ЗапросВМассивОбъектов);</Текст>
		</Алгоритм>
		<Алгоритм Имя="ДобавитьПараметры" ИспользуетсяПриЗагрузке="false">
			<Текст>МенеджерВременныхТаблиц = Новый МенеджерВременныхТаблиц;

ВходВТ_Договоры = Новый Структура("МенеджерВременныхТаблиц", МенеджерВременныхТаблиц);
Выполнить(Алгоритмы.ВТ_Договоры);

Запрос = Новый Запрос;
Запрос.МенеджерВременныхТаблиц = МенеджерВременныхТаблиц;

Запрос.Текст = "
|ВЫБРАТЬ РАЗЛИЧНЫЕ
